		"build:clean": "rm -r ./dist/*",
		"test": "jest",
		"type": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ikalco/ChessGame.git"
	},
	"author": "ikalco",
	"license": "ISC",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/p5": "^1.7.6",
		"jest": "^29.7.0",
		"p5": "^1.9.0",
		"ts-jest": "^29.1.1",
		"typescript": "^5.3.3"
	}
}
